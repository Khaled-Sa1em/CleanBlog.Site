@inherits Umbraco.Web.Mvc.UmbracoViewPage


@using ClientDependency.Core.Mvc
@*@using Our.Umbraco.EditLink*@

@{
    Layout = null;

    Html.RequiresJs("https://use.fontawesome.com/releases/v6.3.0/js/all.js");

    Html.RequiresCss("https://fonts.googleapis.com/css?family=Lora:400,700,400italic,700italic");
    Html.RequiresCss("https://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,800italic,400,300,600,700,800");
    Html.RequiresCss("/css/bootstrap.min.css");
    Html.RequiresCss("/css/styles.css");
    Html.RequiresCss("/css/custom.css");

    //lazy loading for images
    Html.RequiresJs("/scripts/lazysizes.min.js", new { async = "async" });
    Html.RequiresJs("/scripts/ls.unveilhooks.min.js", new { async = "async" });

    //Html.RequiresJs("https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.4/jquery.min.js"); // Latest stable version
    Html.RequiresJs("/scripts/jquery-3.7.1.min.js");
    //Html.RequiresJs("https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js");
    //Html.RequiresJs("/scripts/bootstrap.bundle.min.js");

    //Html.RequiresJs("https://cdn.startbootstrap.com/sb-forms-latest.js");
    Html.RequiresJs("/js/scripts.js");
}
<!DOCTYPE html>
<html lang="en">
<head>
    @Html.Partial("~/Views/Partials/_metaData.cshtml")
    <link rel="icon" type="image/x-icon" href="/assets/favicon.ico" />
    @Html.RenderCssHere()
</head>
<body>
    @Html.Partial("_navigation")
    @Html.Partial("_header")
    <div class="container px-4 px-lg-5">
        @RenderBody()
    </div>
    @Html.Partial("_footer")
    @Html.RenderJsHere()
</body>
</html>
